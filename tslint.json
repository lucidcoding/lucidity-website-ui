{
  "extends": ["tslint:latest"],
  "rules": {
    "import-name": false,
    "variable-name": {
      "options": ["ban-keywords", "check-format", "allow-leading-underscore", "allow-pascal-case"]
    },
    "no-console": false,
    "no-shadowed-variable": false,
    "unnecessary-bind": false,
    "no-implicit-dependencies": [true, "dev"]
  }
}
